{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"version": 3,
	"customTemplates": [
		{
			"name": "no-title",
			"title": "Pages: No Title",
			"postTypes": [ "page" ]
		},
		{
			"name": "blank-canvas",
			"title": "Pages: Blank Canvas",
			"postTypes": [ "page" ]
		}
	],
	"templateParts": [
		{
			"name": "header",
			"area": "header",
			"title": "Header"
		},
		{
			"name": "footer",
			"area": "footer",
			"title": "Footer"
		},
		{
			"name": "comments",
			"area": "comments",
			"title": "Comments"
		},
		{
			"name": "sidebar",
			"area": "sidebar",
			"title": "Sidebar"
		},
		{
			"name": "products",
			"area": "products",
			"title": "Products Grid"
		}
	],
	"settings": {
		"custom": {
			"border-radius": {
				"small": "3px",
				"medium": "12px",
				"large": "24px"
			},
			"button-radius": "var(--wp--custom--border-radius--large)",
			"input-radius": "var(--wp--custom--border-radius--large)",
			"select-radius": "var(--wp--custom--border-radius--large)",
			"group-hover-bg": "color-mix(in srgb, var(--wp--preset--color--base) 85%, transparent)",
			"pagination-circle-bg": "var(--wp--preset--color--accent-1)",
			"shift": {
				"left": {
					"small": "-4vw",
					"medium": "-8vw",
					"large": "-12vw"
				},
				"right": {
					"small": "4vw",
					"medium": "8vw",
					"large": "12vw"
				}
			},
			"wc-bold-text": "600",
			"wc-price-sale": "var(--wp--preset--color--accent-1)",
			"wc-sale-letter-case": "uppercase",
			"wc-sale-bg": "var(--wp--preset--color--accent-2)",
			"wc-sale-color": "var(--wp--preset--color--base)",
			"wc-sale-border": "none",
			"wc-sale-border-radius": "99999px",
			"wc-sale-padding": ".25em .75em",
			"wc-sale-display": "flex",
			"wc-sale-aspect-ratio": "1",
			"wc-sale-position-top": "4px",
			"wc-sale-position-left": "4px",
			"wc-sale-position-right": "4px",
			"wc-sale-letter-case-single": "var(--wp--custom--wc-sale-letter-case)",
			"wc-sale-bg-single": "var(--wp--custom--wc-sale-bg)",
			"wc-sale-color-single": "var(--wp--custom--wc-sale-color)",
			"wc-sale-border-radius-single": "var(--wp--custom--wc-sale-border-radius)",
			"wc-sale-position-top-single": "var(--wp--custom--wc-sale-position-top)",
			"wc-sale-position-left-single": "var(--wp--custom--wc-sale-position-left)"
		},
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "672px",
			"wideSize": "1344px"
		},
		"color": {
			"defaultPalette": true,
			"defaultGradients": true,
			"palette": [
				{
					"slug": "contrast",
					"name": "Contrast",
					"color": "#1d2327"
				},
				{
					"slug": "base",
					"name": "Base",
					"color": "#ffffff"
				},
				{
					"slug": "accent-1",
					"name": "Accent 1",
					"color": "#2d59f2"
				},
				{
					"slug": "accent-1-transparent",
					"name": "Accent 1 Transparent",
					"color": "#2d59f20f"
				},
				{
					"slug": "accent-2",
					"name": "Accent 2",
					"color": "#87c826"
				},
				{
					"slug": "accent-3",
					"name": "Accent 3",
					"color": "#da7021"
				},
				{
					"slug": "contrast-2",
					"name": "Contrast 2",
					"color": "#2d3336"
				},
				{
					"slug": "contrast-3",
					"name": "Contrast 3",
					"color": "#636363"
				},
				{
					"slug": "base-2",
					"name": "Base 2",
					"color": "#f9f9f9"
				},
				{
					"slug": "base-3",
					"name": "Base 3",
					"color": "#e8e8e8"
				}
			],
			"gradients": [
				{
					"slug": "contrast-and-accent-1",
					"name": "Contrast and Accent 1",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--contrast) 0%, var(--wp--preset--color--contrast) 67%, var(--wp--preset--color--accent-1) 67%, var(--wp--preset--color--accent-1) 100%)"
				},
				{
					"slug": "contrast-and-accent-2",
					"name": "Contrast and Accent 2",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--contrast) 0%, var(--wp--preset--color--contrast) 67%, var(--wp--preset--color--accent-2) 67%, var(--wp--preset--color--accent-2) 100%)"
				},
				{
					"slug": "contrast-and-accent-3",
					"name": "Contrast and Accent 3",
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--contrast) 0%, var(--wp--preset--color--contrast) 67%, var(--wp--preset--color--accent-3) 67%, var(--wp--preset--color--accent-3) 100%)"
				},
				{
					"slug": "accent-to-accent-2",
					"name": "Accent to Accent 2",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--accent-1) 0%, var(--wp--preset--color--accent-2) 100%)"
				},
				{
					"slug": "accent-2-to-accent-1",
					"name": "Accent 2 to Accent 1",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--accent-2) 0%, var(--wp--preset--color--accent-1) 100%)"
				},
				{
					"slug": "accent-to-accent-3",
					"name": "Accent to Accent 3",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--accent-1) 0%, var(--wp--preset--color--accent-3) 100%)"
				},
				{
					"slug": "accent-3-to-accent-1",
					"name": "Accent 3 to Accent 1",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--accent-3) 0%, var(--wp--preset--color--accent-1) 100%)"
				},
				{
					"slug": "accent-2-to-accent-3",
					"name": "Accent 2 to Accent 3",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--accent-2) 0%, var(--wp--preset--color--accent-3) 100%)"
				},
				{
					"slug": "accent-3-to-accent-2",
					"name": "Accent 3 to Accent 2",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--accent-3) 0%, var(--wp--preset--color--accent-2) 100%)"
				},
				{
					"slug": "base-to-base-2",
					"name": "Base to Base 2",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--base) 0%, var(--wp--preset--color--base-2) 100%)"
				},
				{
					"slug": "contrast-3-to-contrast",
					"name": "Contrast 3 to Contrast",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--contrast-3) 0%, var(--wp--preset--color--contrast) 100%)"
				}
			],
			"duotone": [
				{
					"name": "White / Accent 1",
					"slug": "white-and-accent-1",
					"colors": [ "#ffffff", "#2d59f2" ]
				},
				{
					"name": "White / Accent 2",
					"slug": "white-and-accent-2",
					"colors": [ "#ffffff", "#87c826" ]
				},
				{
					"name": "White / Accent 3",
					"slug": "white-and-accent-3",
					"colors": [ "#ffffff", "#da7021" ]
				},
				{
					"name": "Base / Base 2",
					"slug": "base-and-base-2",
					"colors": [ "#ffffff", "#f9f9f9" ]
				},
				{
					"name": "Accent 1 / White",
					"slug": "accent-1-and-white",
					"colors": [ "#2d59f2", "#ffffff" ]
				},
				{
					"name": "Accent 2 / White",
					"slug": "accent-2-and-white",
					"colors": [ "#87c826", "#ffffff" ]
				},
				{
					"name": "Accent 3 / White",
					"slug": "accent-3-and-white",
					"colors": [ "#da7021", "#ffffff" ]
				},
				{
					"name": "Base 2 / Base",
					"slug": "base-2-and-base",
					"colors": [ "#f9f9f9", "#ffffff" ]
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Montserrat\", sans-serif",
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": "file:./assets/fonts/Montserrat-VariableFont_wght.woff2"
						},
						{
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": "file:./assets/fonts/Montserrat-Italic-VariableFont_wght.woff2"
						}
					]
				},
				{
					"fontFamily": "\"Red Hat Mono\", monospace",
					"name": "Red Hat Mono",
					"slug": "red-hat-mono",
					"fontFace": [
						{
							"fontFamily": "Red Hat Mono",
							"fontWeight": "300 700",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"src": "file:./assets/fonts/RedHatMono-VariableFont_wght.woff2"
						},
						{
							"fontFamily": "Red Hat Mono",
							"fontWeight": "300 700",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"src": "file:./assets/fonts/RedHatMono-Italic-VariableFont_wght.woff2"
						}
					]
				}
			],
			"fluid": true,
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"slug": "small",
					"name": "Small",
					"size": "14px",
					"fluid": {
						"min": "12px",
						"max": "14px"
					}
				},
				{
					"slug": "medium",
					"name": "Medium",
					"size": "16px",
					"fluid": {
						"min": "14px",
						"max": "16px"
					}
				},
				{
					"slug": "large",
					"name": "Large",
					"size": "24px",
					"fluid": {
						"min": "20px",
						"max": "24px"
					}
				},
				{
					"slug": "x-large",
					"name": "XL",
					"size": "36px",
					"fluid": {
						"min": "24px",
						"max": "36px"
					}
				},
				{
					"slug": "xx-large",
					"name": "XXL",
					"size": "48px",
					"fluid": {
						"min": "36px",
						"max": "48px"
					}
				}
			]
		},
		"shadow": {
			"defaultPresets": true
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"top": "0px",
				"right": "var(--wp--preset--spacing--50)",
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent-1)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "600",
					"lineHeight": "1.2"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "calc( var(--wp--preset--font-size--medium) * 1.3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "calc( var(--wp--preset--font-size--medium) * 1.2)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "calc( var(--wp--preset--font-size--medium) * 1.1)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"button": {
				"color": {
					"background": "var:preset|color|accent-1",
					"text": "var:preset|color|base"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|accent-1",
						"text": "var:preset|color|base"
					}
				},
				"border": {
					"radius": "var(--wp--custom--button-radius)"
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--20)"
					}
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6",
			"fontWeight": "normal",
			"fontStyle": "normal"
		},
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"color": {
							"text": "var(--wp--preset--color--accent-1)"
						},
						"border": {
							"top": {
								"color": "currentColor",
								"style": "solid",
								"width": "1px"
							},
							"right": {
								"color": "currentColor",
								"style": "solid",
								"width": "1px"
							},
							"bottom": {
								"color": "currentColor",
								"style": "solid",
								"width": "1px"
							},
							"left": {
								"color": "currentColor",
								"style": "solid",
								"width": "1px"
							}
						},
						"spacing": {
							"padding": {
								"left": "var(--wp--preset--spacing--40)",
								"right": "var(--wp--preset--spacing--40)",
								"top": "calc( var(--wp--preset--spacing--20) - 1px)",
								"bottom": "calc( var(--wp--preset--spacing--20) - 1px)"
							}
						}
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|contrast"
						}
					}
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "1em"
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var:preset|font-size|medium"
				},
				"color": {
					"background": "var:preset|color|base-2"
				}
			},
			"core/pullquote": {
				"color": {
					"background": "var:preset|color|base-2"
				},
				"typography": {
					"fontSize": "var:preset|font-size|medium"
				}
			},
			"core/code": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--red-hat-mono)",
					"fontSize": "var:preset|font-size|medium",
					"fontWeight": "400"
				},
				"color": {
					"background": "var:preset|color|base-2",
					"text": "var:preset|color|contrast"
				}
			},
			"core/separator": {
				"color": {
					"background": "var:preset|color|contrast"
				}
			},
			"core/navigation-submenu": {
				"css": ".has-background {\n  padding: 0;\n}"
			},
			"core/template-part": {
				"css": "margin-top: 0"
			},
			"core/table": {
				"css": "& thead {\n  border-bottom: 2px solid var(--wp--preset--color--base-3);\n}\n& tfoot {\n  border-top: 2px solid var(--wp--preset--color--base-3);\n}\n& th {\n  border-color: var(--wp--preset--color--base-3);\n}\n& td {\n  border-color: var(--wp--preset--color--base-3);\n}\n& th:not(.has-text-align-center):not(.has-text-align-right) {\n  text-align: left;\n}"
			},
			"core/search": {
				"css": ".wp-block-search__button-inside .has-icon.wp-element-button {\n  padding: calc(0.333em + 2px) calc(0.333em + 4px) calc(0.333em + 2px) calc(0.333em + 2px) !important;\n}\n& .wp-block-search__input,\n.wp-block-search__inside-wrapper {\n  border-color: var(--wp--preset--color--base-3);\n  border-radius: var(--wp--custom--input-radius);\n}\n& .wp-element-button {\n  padding: calc(0.333em + 2px) calc(0.667em + 2px);\n  border-color: transparent !important;\n  border-radius: var(--wp--custom--input-radius);\n}\n& .wp-block-search__button {\n  border-radius: var(--wp--custom--input-radius) !important;\n}"
			}
		},
		"css": ".wp-block-navigation:not(.has-text-color) .wp-block-navigation__submenu-container {\n  color: var(--wp--preset--color--contrast);\n}\n.wp-block-navigation:not(.has-background) .wp-block-navigation__submenu-container {\n  background-color: var(--wp--preset--color--base);\n  border: none;\n}\nol.wp-block-latest-comments {\n  padding-left: 0;\n}"
	}
}